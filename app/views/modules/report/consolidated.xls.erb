<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet" xmlns:o="urn:schemas-microsoft-com:office:office" xmlns:x="urn:schemas-microsoft-com:office:excel" xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet" xmlns:html="http://www.w3.org/TR/REC-html40">
	<DocumentProperties xmlns="urn:schemas-microsoft-com:office:office">
		<LastAuthor><%=current_admin.fullname%></LastAuthor>
		<Created><%=Date.today%></Created>
		<Version>1.00</Version>
	</DocumentProperties>
	<OfficeDocumentSettings xmlns="urn:schemas-microsoft-com:office:office">
		<AllowPNG/>
		<Colors>
			<Color>
				<Index>8</Index>
				<RGB>#7E0021</RGB>
			</Color>
		</Colors>
	</OfficeDocumentSettings>
	<ExcelWorkbook xmlns="urn:schemas-microsoft-com:office:excel">
		<WindowHeight>8190</WindowHeight>
		<WindowWidth>16380</WindowWidth>
		<WindowTopX>0</WindowTopX>
		<WindowTopY>0</WindowTopY>
		<TabRatio>198</TabRatio>
		<ProtectStructure>False</ProtectStructure>
		<ProtectWindows>False</ProtectWindows>
	</ExcelWorkbook>
	<Styles>
		<Style ss:ID="Default" ss:Name="Normal">
			<Alignment ss:Vertical="Bottom"/>
			<Borders/>
			<Font ss:FontName="Arial" x:Family="Swiss"/>
			<Interior/>
			<NumberFormat/>
			<Protection/>
		</Style>
		<Style ss:ID="s111">
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="11"/>
		</Style>
		<Style ss:ID="sgeneric">
			<Alignment ss:Vertical="Bottom" ss:WrapText="1"/>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="11"/>
		</Style>
		<Style ss:ID="snumber">
			<Alignment ss:Horizontal="Center" ss:Vertical="Center" ss:WrapText="1"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="11"/>
		</Style>
		<Style ss:ID="sblack_cel">
			<Alignment ss:Horizontal="Center" ss:Vertical="Center"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="11" ss:Color="#FFFFFF"/>
			<Interior ss:Color="#848484" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="sorange_cel">
			<Alignment ss:Horizontal="Center" ss:Vertical="Center"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="11" ss:Color="#454545"/>
			<Interior ss:Color="#FFC657" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="sred_cel">
			<Alignment ss:Horizontal="Center" ss:Vertical="Center"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="11" ss:Color="#FFFFFF"/>
			<Interior ss:Color="#E04141" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="stitle">
			<Alignment ss:Vertical="Bottom"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="12" ss:Bold="1" ss:Color="#FFFFFF"/>
			<Interior ss:Color="#848484" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="stitle_ini">
			<Alignment ss:Vertical="Bottom"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="12" ss:Bold="1" ss:Color="#FFFFFF"/>
			<Interior ss:Color="#848484" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="stitle_fin">
			<Alignment ss:Vertical="Bottom"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="12" ss:Bold="1" ss:Color="#FFFFFF"/>
			<Interior ss:Color="#848484" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="sDotname">
			<Alignment ss:Horizontal="Left" ss:Vertical="Center" ss:WrapText="1"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="11"/>
		</Style>
		<Style ss:ID="stitlered">
			<Alignment ss:Vertical="Center" ss:WrapText="1"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="12" ss:Bold="1"/>
			<Interior ss:Color="#DC1818" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="stitlered_ini">
			<Alignment ss:Vertical="Bottom"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="12" ss:Bold="1" ss:Color="#FFFFFF"/>
			<Interior ss:Color="#DC1818" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="stitlered_fin">
			<Alignment ss:Vertical="Bottom"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="12" ss:Bold="1" ss:Color="#FFFFFF"/>
			<Interior ss:Color="#DC1818" ss:Pattern="Solid"/>
		</Style>
		<Style ss:ID="sgray_col">
			<Alignment ss:Horizontal="Center" ss:Vertical="Bottom" ss:WrapText="1"/>
			<Borders>
				<Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
				<Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
			</Borders>
			<Font ss:FontName="Calibri" x:Family="Swiss" ss:Size="11" ss:Bold="1" ss:Color="#000000"/>
			<Interior ss:Color="#ECECEC" ss:Pattern="Solid"/>
		</Style>
	</Styles>
	<Worksheet ss:Name="Dots Consolidado <%=@country.country.name%>">
		<Table x:FullColumns="1" x:FullRows="1" ss:StyleID="s111" ss:DefaultColumnWidth="60.75" ss:DefaultRowHeight="15">
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="131.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="73.25"/>
			<Column ss:StyleID="s111" ss:AutoFitWidth="0" ss:Width="120.25"/>
			

				<%@country.country.talent_hunters.for_report.order("talent_hunters.created_at ASC").each do |hunter| %>	
					<Row>
					  <Cell ss:Index="2" ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					  <Cell ss:StyleID="sgeneric"/>
					</Row>
					
					<Row ss:AutoFitHeight="0">
						<Cell ss:StyleID="stitle_ini">
							<Data ss:Type="String"><%=@country.country.name %> - <%=hunter.full_name %> </Data>
						</Cell>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle"/>
						<Cell ss:StyleID="stitle_fin"/>
					</Row>

					<Row>
						<Cell ss:StyleID="sgray_col">
							<Data ss:Type="String">DOTS</Data>
						</Cell>

						<% (1..4).each do |i| %>
							<Cell ss:StyleID="sgray_col">
								<Data ss:Type="String">Trimestre <%=i%></Data>
							</Cell>
							<Cell ss:StyleID="sgray_col">
								<Data ss:Type="String">Promedio T<%=i%></Data>
							</Cell>
						<% end %>

						<Cell ss:StyleID="sblack_cel">
							<Data ss:Type="String">TOTAL</Data>
						</Cell>
						<Cell ss:StyleID="sblack_cel">
							<Data ss:Type="String">META</Data>
						</Cell>
						<Cell ss:StyleID="sblack_cel">
							<Data ss:Type="String">PROMEDIO (<%=@current_timeframes.where("quarter between 1 and ?",@current_timeframe.quarter).count%> Semanas)</Data>
						</Cell>
					</Row>

					<%@country.countries_dots.order("countries_dots.dot_id ASC").each do |dot| %>
						<%total_dot=0%>
						<%goal_dot=0%>
						<%goal_dot=@country.country.talent_hunters.for_report.order("talent_hunters.created_at ASC").first.talent_hunters_dots.order("id ASC").where(:dot_id=>dot.dot_id).first %>
						<Row>
		    				<Cell ss:StyleID="sDotname">
								<Data ss:Type="String"><%=dot.dot.name%></Data>
							</Cell>
							<% (1..4).each do |i| %>
								<% hunter.talent_hunters_dots.order("id ASC").where(:dot_id=>dot.dot_id).each do |goal| %>
									<% dot_sc=0 %>
									<% dot_avg=0 %>
									
										<% scored=goal.dots_scoreds.includes(:work_timeframe).where("work_timeframes.is_disabled=false and work_timeframes.quarter=#{i}") %>
										<%dot_sc=scored.sum("total")%>
										<%dot_avg=scored.average("total")%>
										<Cell ss:StyleID="snumber"><Data ss:Type="Number"><%=dot_sc > 0 ? dot_sc : '0'%></Data></Cell>
										<Cell ss:StyleID="snumber"><Data ss:Type="Number"><%=dot_avg > 0 ? dot_avg.round : '0'%></Data></Cell>
										<%total_dot+=scored.sum("total")%>
									
								<% end %>
							<% end %>
							<Cell ss:StyleID="sblack_cel"><Data ss:Type="Number"><%=total_dot%></Data></Cell>
							<Cell ss:StyleID="sblack_cel"><Data ss:Type="Number"><%=goal_dot.goal%></Data></Cell>
							<Cell ss:StyleID="sblack_cel"><Data ss:Type="Number"><%=((total_dot.to_f)/@current_timeframes.where("quarter between 1 and ?",@current_timeframe.quarter).count).round%></Data></Cell>
						</Row>
					<%end %>
				<% end %>

		</Table>

		<WorksheetOptions xmlns="urn:schemas-microsoft-com:office:excel">
			<PageSetup>
				<Layout x:StartPageNumber="1"/>
				<Header x:Margin="0.78749999999999998" x:Data="&C&A"/>
				<Footer x:Margin="0.78749999999999998" x:Data="&CPágina &P"/>
				<PageMargins x:Bottom="1.0249999999999999" x:Left="0.78749999999999998" x:Right="0.78749999999999998" x:Top="1.0249999999999999"/>
			</PageSetup>
			<Print>
				<ValidPrinterInfo/>
				<HorizontalResolution>300</HorizontalResolution>
				<VerticalResolution>300</VerticalResolution>
			</Print>
			<Zoom>85</Zoom>
			<Selected/>
			
			<ProtectObjects>False</ProtectObjects>
			<ProtectScenarios>False</ProtectScenarios>
			<EnableSelection>NoSelection</EnableSelection>
		</WorksheetOptions>
	</Worksheet>
</Workbook>